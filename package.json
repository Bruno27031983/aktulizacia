  "description": "Progressive Web Application pre sledovanie pracovného času a výpočet mzdy",
  "main": "index.html",
  "scripts": {
    "test": "node test-app.js",
    "test:calculations": "node test-calculations.js",
    "test:integrations": "node test-integrations.js",
    "test:offline": "node test-integrations-offline.js",
    "test:all": "npm test && npm run test:calculations && npm run test:integrations && npm run test:offline",
    "serve": "npx http-server -p 8080 -c-1",
    "serve:https": "npx http-server -p 8443 -S -C cert.pem -K key.pem -c-1"
